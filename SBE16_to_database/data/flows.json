    {
        "id": "2fea6077.dcd2a",
        "type": "function",
        "z": "3722000.5fe76",
        "name": "parse ctd data string",
        "func": "var parts = msg.payload.ctd.trim().split(/\\s+/);\n    if(parts.length < 6){\n      //error.write(records[i],\"not enough fields\");\n      return null;\n    }\n\t\n//|  24.94  14.498  40.174  32.926 1503.0553 14:58:00.92M\t\n\t\nvar timestamp =  msg.payload.mi_timestamp;\nvar instrument_id = msg.payload.instrument_id;\nvar raw = msg.payload.ctd.trim();\nvar press = parseFloat(parts[0]);\nvar temp =  parseFloat(parts[1]);\nvar cond = parseFloat(parts[2]);\nvar sal = parseFloat(parts[3]);\nvar soundv = parseFloat(parts[4]);\nvar dissolved_oxygen_sat = parseFloat(parts[5]);\nvar dissolved_oxygen_ppm = parseFloat(parts[6]);\nvar ttime = parts[7];\nvar newMsg = {\n       payload: {\n\t    time: timestamp,\n\t\tinstrument_id: instrument_id,\n\t\traw: raw,\n\t\tpress: press,\n\t\ttemp: temp,\n\t\tcond: cond,\n\t\tsal: sal,\n\t\tsoundv: soundv,\n\t\tttime: ttime,\n\t\tdissolved_oxygen_ppm: dissolved_oxygen_ppm,\n\t\tdissolved_oxygen_sat: dissolved_oxygen_sat\n\t   }\n    };\nreturn newMsg;",
        "outputs": 1,
        "noerr": 0,
        "x": 756,
        "y": 209,
        "wires": [
            [
                "cc9a7251.33ee9"
            ]
        ]
    },
